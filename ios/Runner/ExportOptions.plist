<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- distribution method: app-store / ad-hoc / development / enterprise -->
    <key>method</key>
    <string>development</string>

    <!-- salt removal to reduce IPA size -->
    <key>stripSwiftSymbols</key>
    <true/>

    <!-- 控制 App Thinning，通常保留默认 -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>

    <!-- Xcode 打包目标 -->
    <key>destination</key>
    <string>export</string>

    <!-- 签名方式 -->
    <key>signingStyle</key>
    <string>manual</string>

    <!-- 默认为 Apple Distribution，若使用 development 可改为 Apple Development -->
    <key>signingCertificate</key>
    <string>Apple Distribution</string>

    <!-- 应用团队的 Team ID -->
    <key>teamID</key>
    <string>Mutse Young</string>

    <!-- 手动指定 provisioning profile，bundle ID 用你的替换 -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.mutse.chibot</key>
        <string>Your Provisioning Profile Name</string>
    </dict>
</dict>
</plist>